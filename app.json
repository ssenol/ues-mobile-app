{
  "expo": {
    "entryPoint": "./App.js",
    "name": "ues-speech-app",
    "slug": "ues-speech-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "background": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSFaceIDUsageDescription": "Bu uygulama Face ID ile giriş yapmanıza izin verir.",
        "ITSAppUsesNonExemptEncryption": false
      },
      "bundleIdentifier": "com.ues.uesspeechapp",
      "buildNumber": "1.0.1"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "background": "#ffffff"
      },
      "package": "com.ues.uesspeechapp"
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "8d7d268f-f363-4dc8-9657-b9ee544b79a7"
      }
    },
    "owner": "selcuksenol",
    "plugins": [
      "expo-secure-store"
    ]
  }
}
